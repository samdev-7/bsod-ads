{
  "manifest_version": 3,
  "name": "BSOD Ads",
  "version": "1.0.0",
  "description": "Why ads when they can show up as a BSOD instead",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "author": "hello@samliu.dev",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
